{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/docs",
      "dest": "/docs/index.html"
    },
    {
      "src": "/api-docs.json",
      "dest": "/index.js"
    }
  ],

  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
