{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "rewrites": [{ "source": "/docs", "destination": "/docs/index.html" }],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
